{
  "name": "dev-site-components",
  "version": "1.0.4",
  "main": "dist/node/remote-entry.js",
  "browser": "dist/browser/remote-entry.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build:dev": "webpack --mode development",
    "watch:dev": "webpack --mode development --watch",
    "test": "jest --json --outputFile=.jest-test-results.json || true",
    "test:watch": "jest --json --outputFile=.jest-test-results.json --watch || true",
    "start": "webpack serve --open --mode development",
    "start:live": "webpack serve --open --mode development --liveReload",
    "story": "rm -rf ./dist && concurrently -k \"yarn watch:dev\" \"yarn storybook\" \"yarn federate\"",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "federate": "serve ./dist -p 3003",
    "setup": "yarn && yarn test",
    "generate-component": "node ./src/util/cli/generateComponent/index.ts && yarn test "
  },
  "license": "MIT",
  "author": {
    "name": "Austin Howard",
    "email": "ahoward@austinhoward.tech"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-jest": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "babel-loader": "^8.2.2",
    "camel-case": "^4.1.2",
    "concurrently": "^6.2.1",
    "css-loader": "^3.5.3",
    "inquirer": "^8.1.4",
    "jest": "^27.2.0",
    "json-loader": "^0.5.7",
    "marked": "^3.0.4",
    "serve": "^12.0.1",
    "storybook-addon-performance": "^0.16.1",
    "storybook-readme": "^5.0.9",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.1"
  }
}
